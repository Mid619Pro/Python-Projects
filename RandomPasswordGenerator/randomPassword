import random

print("Welcome To Password Generator")
randomCharacters="ABCDEFGHIJKLMNOPQRSTUVWXYZqwertyuioplkjhgfdsazxcvbnm1234567890@!#$%?"
passwordLength=int(input("Enter the Length of Character: "))
NumOfPass=int(input("Enter How Many Passwords You Need: "))
print("Here are the Passwords")

for x in range(NumOfPass):
    pwd=""
    for i in range(passwordLength):
        pwd=pwd+random.choice(randomCharacters)
    print(pwd)